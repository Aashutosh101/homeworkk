<%= $webhook.inspect %>
<%= Time.now.utc.strftime("%H:%M:%S") %>
<%

	@groups = Group.all
	@groups.each do |group|
		if group.end_time.strftime("%H:%M:%S") >= Time.now.utc.strftime("%H:%M:%S")
			Messagehuman.sendMessage(group.conversation_id, group.name)
			puts "trying the heroku scheduler"
		end
	end

%>