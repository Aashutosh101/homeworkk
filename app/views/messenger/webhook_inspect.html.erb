<%= $webhook.inspect %>
<%

	@groups = Group.all
	t = Time.now
	timeten = 10.minutes.from_now
	@groups.each do |group|
		if group.end_time.utc.strftime("%H:%M:%S") >= t.utc.strftime("%H:%M:%S") && group.end_time.utc.strftime("%H:%M:%S") <=  timeten.utc.strftime("%H:%M:%S")
			%>
			<%= "stuff: " + group.end_time.utc.strftime("%H:%M:%S") %> <%
			Messagehuman.sendMessage(group.conversation_id, group.name)
			puts "trying the heroku scheduler"
		end
	end

%>
<%= timeten.utc.strftime("%H:%M:%S") %> <br><br>
<%= Time.now + 10*60 %>